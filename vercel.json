{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.py",
            "use": "@vercel/python"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/sdes",
            "dest": "/api/sdes.py"
        },
        {
            "src": "/api/rsa",
            "dest": "/api/rsa.py"
        },
        {
            "src": "/api/hill",
            "dest": "/api/hill.py"
        },
        {
            "src": "/api/playfair",
            "dest": "/api/playfair.py"
        },
        {
            "src": "/api/monoalphabetic",
            "dest": "/api/monoalphabetic.py"
        },
        {
            "src": "/api/adfgvx",
            "dest": "/api/adfgvx.py"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}