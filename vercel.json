{
    "version": 2,
    "builds": [
        {
            "src": "api/*.py",
            "use": "@vercel/python"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/co1",
            "dest": "/api/co1.py"
        },
        {
            "src": "/api/co2",
            "dest": "/api/co2.py"
        },
        {
            "src": "/api/math-ops",
            "dest": "/api/math-ops.py"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}